<launch>
  <!-- Arguments -->

  <arg name="map_file" default="$(find turtlebot3_navigation)/maps/map.yaml"/>
  
 <node type="rviz" name="rviz" pkg="rviz" 
	args="-d $(find final_design_2)/rviz/navigation_rviz.rviz" />
  

  <!-- Map server -->
  <node pkg="map_server" name="map_server" type="map_server" args="$(arg map_file)"/>

  <!-- AMCL -->
  <include file="$(find final_design_2)/launch/amcl.launch"/>

  <!-- Move base -->
  <include file="$(find final_design_2)/launch/move_base.launch">
    
  </include>

</launch>
